<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Comment</key>
	<string>Git repo: https://github.com/RocketChat/Rocket.Chat.Electron</string>
	<key>Description</key>
	<string>Downloads the latest version of Rocket.Chat Desktop App for Mac OSX.</string>
	<key>Identifier</key>
	<string>com.github.homebysix.install.RocketChat</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>RocketChat</string>
	</dict>
	<key>MinimumVersion</key>
	<string>0.5.0</string>
	<key>ParentRecipe</key>
	<string>com.github.hutr.download.RocketChat</string>
	<key>Process</key>
	<array>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>dmg_path</key>
				<string>%pathname%</string>
				<key>items_to_copy</key>
				<array>
					<dict>
						<key>destination_path</key>
						<string>/Applications</string>
						<key>source_item</key>
						<string>Rocket.Chat.app</string>
					</dict>
				</array>
			</dict>
			<key>Processor</key>
			<string>InstallFromDMG</string>
		</dict>
	</array>
</dict>
</plist>
