<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads the latest version of FortiClient.</string>
	<key>Identifier</key>
	<string>com.github.hutr.download.FortiClientUpdate</string>
	<key>Input</key>
	<dict>
		<key>MAJOR_VERSION</key>
		<string>11</string>
		<key>OS_VERSION</key>
		<string>10.9</string>
		<key>NAME</key>
		<string>FortiClientUpdate</string>
		<key>DOWNLOAD_URL</key>
		<string>https://storage.googleapis.com/munki-forticlient/FortiClient5.6.1.723.mpkg</string>
	</dict>
	<key>MinimumVersion</key>
	<string>0.2.0</string>
	<key>Process</key>
	<array>
			<dict>
				<key>Arguments</key>
				<dict>
						<key>major_version</key>
						<string>%MAJOR_VERSION%</string>
						<key>os_version</key>
						<string>%OS_VERSION%</string>
				</dict>
					<key>Processor</key>
					<string>URLDownloader</string>
					<key>Arguments</key>
					<dict>
							<key>url</key>
							<string>%DOWNLOAD_URL%</string>
							<key>filename</key>
							<string>%NAME%.mpkg</string>
					</dict>
			</dict>
			<dict>
					<key>Processor</key>
					<string>EndOfCheckPhase</string>
			</dict>
	</array>
</dict>
</plist>
